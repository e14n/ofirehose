{
    "name": "ofirehose",
    "version": "0.2.0-alpha.1",
    "dependencies": {
        "express": "2.5.x",
        "underscore": "1.4.x",
        "utml": "0.2.x",
        "databank": "0.18.x"
    },
    "scripts": {
        "pretest": "coffee -o lib/ -c src/*.coffee",
        "test": "nyc -r lcov perjury test/*-test.js",
        "posttest": "rm -f lib/*.js",
        "build": "coffee -b -c -o lib/ src/*.coffee",
        "prepublish": "mkdir -p lib/ && coffee -b -c -o lib/ src/*.coffee",
        "postpublish": "rm lib/*.js",
        "clean": "rm -f lib/*.js *~ lib/*~ src/*~ test/*~"
    },
    "devDependencies": {
        "coffee-script": "1.6.x",
        "coveralls": "^3.0.0",
        "nyc": "^11.3.0",
        "perjury": "^1.1.0"
    }
}
